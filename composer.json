{
  "name": "wundii/data-mapper-laravel-package",
  "type": "library",
  "description": "A Laravel integration for wundii/data-mapper, modern and fast object mapper for PHP 8.2+. Strict types. Converts JSON, XML, YAML, NEON, arrays, and objects to PHP objects.",
  "keywords": ["laravel","laravel-package","package","datamapper","mapper","xml","json","array","object", "deserialization", "deserialize"],
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas Wunderwald",
      "email": "wundii@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "ext-json": "*",
    "ext-pcre": "*",
    "ext-reflection": "*",
    "ext-simplexml": "*",
    "ext-spl": "*",
    "ext-xml": "*",
    "illuminate/config": "^10.0 || ^11.0 || ^12.0",
    "illuminate/console": "^10.0 || ^11.0 || ^12.0",
    "illuminate/container": "^10.0 || ^11.0 || ^12.0",
    "illuminate/http": "^10.0 || ^11.0 || ^12.0",
    "illuminate/support": "^10.0 || ^11.0 || ^12.0",
    "wundii/data-mapper": "dev-core-refactoring || ^1.3.1"
  },
  "require-dev": {
    "ext-dom": "*",
    "ext-libxml": "*",
    "nette/neon": "^v3.4",
    "orchestra/testbench": "^10.0",
    "phpstan/phpstan": "^2.0",
    "phpstan/phpstan-strict-rules": "^2.0",
    "phpunit/phpunit": "^11.4",
    "rector/rector": "2.0.19",
    "symfony/var-dumper": "^7.0",
    "symfony/yaml": "^7.0",
    "symplify/easy-coding-standard": "^12.3",
    "wundii/phplint": "^0.3"
  },
  "autoload": {
    "psr-4": {
      "Wundii\\DataMapper\\LaravelPackage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wundii\\DataMapper\\LaravelPackage\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Wundii\\DataMapper\\LaravelPackage\\DataMapperServiceProvider"
      ],
      "aliases": {
        "DataMapper": "Wundii\\DataMapper\\LaravelPackage\\Facades\\DataMapper"
      }
    }
  },
  "scripts": {
    "auto-scripts": {
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "complete-check": [
      "@cache-clear",
      "@phplint",
      "@rector-dry",
      "@phpstan",
      "@ecs-dry",
      "@unittest"
    ],
    "cache-clear": "rm -R ./cache/* -f",
    "coverage": "php vendor/bin/phpunit --coverage-clover coverage.xml",
    "ecs-apply": "php vendor/bin/ecs check --fix",
    "ecs-dry": "php vendor/bin/ecs check",
    "phplint": "php vendor/bin/phplint",
    "phpstan": "php vendor/bin/phpstan analyse",
    "rector-apply": "php vendor/bin/rector process --ansi",
    "rector-dry": "php vendor/bin/rector process --dry-run --ansi",
    "unittest": "php vendor/bin/phpunit --configuration phpunit.xml"
  },
  "minimum-stability": "stable"
}