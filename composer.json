{
  "name": "wundii/data-mapper-laravel-package",
  "type": "library",
  "description": "A Laravel Package for modern deserialization and mapping of XML, JSON and arrays in PHP classes - optimized for PHP 8.2+",
  "keywords": ["laravel","laravel-package","package","datamapper","mapper","xml","json","array","object", "deserialization", "deserialize"],
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas Wunderwald",
      "email": "wundii@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "ext-json": "*",
    "ext-pcre": "*",
    "ext-reflection": "*",
    "ext-simplexml": "*",
    "ext-spl": "*",
    "ext-xml": "*",
    "laravel/framework": "v10.0 || v11.0 || v12.0",
    "illuminate/console": "^10.0 || ^11.0 || ^12.0",
    "illuminate/config": "^10.0 || ^11.0 || ^12.0",
    "illuminate/http": "^10.0 || ^11.0 || ^12.0",
    "illuminate/support": "^10.0 || ^11.0 || ^12.0",
    "wundii/data-mapper": "1.3.1"
  },
  "require-dev": {
    "ext-dom": "*",
    "ext-libxml": "*",
    "nette/neon": "^v3.4",
    "phpstan/phpstan": "^2.0",
    "phpstan/phpstan-strict-rules": "^2.0",
    "phpunit/phpunit": "^11.4",
    "rector/rector": "^2.0",
    "symfony/var-dumper": "^7.0",
    "symfony/yaml": "^7.0",
    "symplify/easy-coding-standard": "^12.3",
    "wundii/phplint": "^0.3"
  },
  "autoload": {
    "psr-4": {
      "Wundii\\DataMapper\\LaravelPackage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wundii\\DataMapper\\LaravelPackage\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Wundii\\DataMapper\\LaravelPackage\\DataMapperServiceProvider"
      ],
      "aliases": {
        "DataMapper": "Wundii\\DataMapper\\LaravelPackage\\Facades\\DataMapper"
      }
    }
  },
  "scripts": {
    "auto-scripts": {
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "complete-check": [
      "@cache-clear",
      "@phplint",
      "@rector-dry",
      "@phpstan",
      "@ecs-dry",
      "@unittest"
    ],
    "rector-dry": [
      "php vendor/bin/rector process --dry-run --ansi"
    ],
    "rector-apply": [
      "php vendor/bin/rector process --ansi"
    ],
    "ecs-dry": [
      "php vendor/bin/ecs check"
    ],
    "ecs-apply": [
      "php vendor/bin/ecs check --fix"
    ],
    "phpstan": [
      "php vendor/bin/phpstan analyse"
    ],
    "unittest": [
      "php vendor/bin/phpunit --configuration phpunit.xml"
    ],
    "cache-clear": [
      "rm -R ./Cache/* -f"
    ],
    "phplint": [
      "php vendor/bin/phplint"
    ],
    "coverage": [
      "php vendor/bin/phpunit --coverage-clover coverage.xml"
    ]
  },
  "minimum-stability": "dev"
}